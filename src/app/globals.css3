.loading-wave {
  /* width: 300px; */
  height: 40px;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}

.loading-bar {
  width: 6px;
  height: 10px;
  margin: 0 5px;
  border-radius: 5px;
  /*animation: loading-wave-animation 1s ease-in-out infinite;*/
  animation: wave 0.6s ease-in-out infinite;

}

.loading-bar:nth-child(1) { animation-delay: 0ms; }
.loading-bar:nth-child(2) { animation-delay: 100ms; }
.loading-bar:nth-child(3) { animation-delay: 200ms; }
.loading-bar:nth-child(4) { animation-delay: 300ms; }


 /*.loading-bar:nth-child(2) {animation-delay: 0.1s;}
.loading-bar:nth-child(3) {animation-delay: 0.2s;}
.loading-bar:nth-child(4) {animation-delay: 0.3s;}
*/
@keyframes loading-wave-animation {
  0% {
    height: 5px;
  }

  50% {
    height: 30px;
  }

  100% {
    height: 5px;
  }
}

